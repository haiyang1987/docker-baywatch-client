{
  "network": {
    "servers": [ "192.168.59.103:5000" ],
    "ssl ca": "/etc/pki/tls/certs/logstash-forwarder.crt",
    "timeout": 15
  },
  "files": [
    {
      "paths": [
        "/elk_ubu/log/nginx/*access*.log"
       ],
      "fields": { "type": "nginx_access" }
    },
    {
      "paths": [
        "/var/log/ser*.log"
       ],
      "fields": { "type": "serf" }
    }
  ]
}
